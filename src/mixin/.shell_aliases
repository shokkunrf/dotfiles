# !/bin/sh
#
alias sudo="sudo "
alias la="ls -la"
alias ks="ls"
alias mkf="touch"
alias ivm="vim"

ghq-peco(){
  cd "$( ghq list -p | peco)"
}
alias sd="ghq-peco"

cd-gopath(){
  dir="$(ls $GOPATH/src | peco)"
  cd "$GOPATH/src/$dir"
}
alias gd="cd-gopath"

alias rm='mv --backup=numbered --target-directory=${HOME}/.Trash/'

alias g='git'

