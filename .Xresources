!! ~/.Xresources
! rxvt-unicode
! xrdb -merge ~/.Xresources

! Xft setting
Xft.dpi:       96
Xft.antialias: true

!!!
! Urxvt Setting
!!!

! font
URxvt.font: xft:monospace-12,\
            xft:DejaVu Sans Mono-10,\
	    xft:IPAGothic
URxvt.letterSpace: -1

! scrollBar
! URxvt.scrollBar:       false
URxvt.scrollBar_right:    true
URxvt.scrollBar_floating: true
URxvt.scrollstyle:        plain

! other
URxvt.cursorBlink:        true
URxvt.cursorUnderline:    true
URxvt.visualBell:         true
URxvt.fading:             10

! Transparency
URxvt*inheritPixmap: true
URxvt*.transparent: true
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
URxvt*.shading: 25

!!!
! Color settings gruvbox
!!!
URxvt.foreground:  #ebdbb2
URxvt.background:  #282828
URxvt.colorIT:     #8ec07c
URxvt.colorBD:     #DC2322
URxvt.colorUL:     #83a598
URxvt.scrollColor: #504945
! Black + DarkGrey
URxvt.color0:  #282828
URxvt.color8:  #928374
! DarkRed + Red
URxvt.color1:  #cc241d
URxvt.color9:  #fb4934
! DarkGreen + Green
URxvt.color2:  #98971a
URxvt.color10: #b8bb26
! DarkYellow + Yellow
URxvt.color3:  #d79921
URxvt.color11: #fabd2f
! DarkBlue + Blue
URxvt.color4:  #458588
URxvt.color12: #83a598
! DarkMagenta + Magenta
URxvt.color5:  #b16286
URxvt.color13: #d3869b
! DarkCyan + Cyan
URxvt.color6:  #689d6a
URxvt.color14: #8ec07c
! LightGrey + White
URxvt.color7:  #a89984
URxvt.color15: #ebdbb2

